{
  "SectionCode": "siaebox",
  "SectionName": "SIAE-BOX",
  "AlertBox": {
    "Type": "info",
    "Text": [
      "This section is meaningful and **mandatory** only for italian installations which are subjected to SIAE fiscal constraints.",
      "",
      "**These parameters are not intended for SnApp backoffice installation. Instead should be configured on the SIAE-BOX webapp**."
    ]
  },
  "ParamGroupList": [
    {
      "Caption": "Box ID",
      "ParamList": [
        {
          "ParamName": "VGS-SIAE_BOX_ID",
          "ParamValue": "85525017-9286-47ca-8e4a-bbcfb9a61e22"
        }
      ],
      "AlertBox": {
        "Type": "info",
        "Text": [
          "SIAE BOX identification number."
        ]
      }
    },
    {
      "Caption": "SnApp instance",
      "ParamList": [
        {
          "ParamName": "VGS-Snapp-URL",
          "ParamValue": "https://xxxxxx.xxx"
        },
        {
          "ParamName": "VGS-EXTERNAL_WORKSTATION_ID",
          "ParamValue": "1ab66a04-aa23-4842-a4cf-06a9ce9677f2"
        },
        {
          "ParamName": "VGS-BACKOFFICE_POLL_TIMEOUT",
          "ParamValue": "5"
        }
      ],
      "AlertBox": {
        "Type": "info",
        "Text": [
          "URL and WorkstationId of the SnApp backoffice production instance"
        ]
      }
    },
    {
      "Caption": "Slot number",
      "ParamList": [
        {
          "ParamName": "VGS-SIAE_SLOT_NUMBER",
          "ParamValue": "5"
        },
        {
          "ParamName": "VGS-SLOT_POLLING_TIMEOUT",
          "ParamValue": "5"
        }
      ],
      "AlertBox": {
        "Type": "info",
        "Text": [
          "Max number of SIAE cards that can be connected to the server."
        ]
      }
    },
    {
      "Caption": "Card reader watchdog",
      "ParamList": [
        {
          "ParamName": "VGS-LECTORS_WATCHDOG_ENABLED",
          "ParamValue": "0"
        },
        {
          "ParamName": "VGS-LECTORS_WATCHDOG_TIMEOUT",
          "ParamValue": "180000"
        }
      ],
      "AlertBox": {
        "Type": "info",
        "Text": [
          "Card polling configuration.",
          "Enabled can be 0 or 1.",
          "Timeout is expressed in milliseconds."
        ]
      }
    }
  ]
