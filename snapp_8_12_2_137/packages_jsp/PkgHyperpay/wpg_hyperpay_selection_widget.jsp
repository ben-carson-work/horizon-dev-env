<%@page import="com.vgs.web.page.PageBO_Base"%>
<%@page import="com.vgs.cl.*"%>
<%@page import="com.vgs.web.tag.*"%>
<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<%@ taglib uri="vgs-tags" prefix="v" %>

<jsp:useBean id="authResponse" class="com.vgs.snapp.dataobject.DOWpgResponse" scope="request"/>

<%
PageBO_Base<?> pageBase = (PageBO_Base<?>)request.getAttribute("pageBase");

String site_url = ConfigTag.getValue("site_url"); 
String saleId = pageBase.getNullParameter("SaleId");


%>

<style>
#hyperpay-body {
  padding: 20px;
}

#hyperpay-body .payment-title {
	font-size: 20px;
	margin-bottom: 20px;
}

#hyperpay-body .payment-list {
  /*padding: 10px;*/
}

#hyperpay-body .payment-entity {
  display: inline-block;
  width: 120px;
  height: 80px;
  background-size: 80%;
  background-repeat: no-repeat;
  background-position: center;
  background-color: white;
  margin: 10px;
  border-radius: 10px;
  border: 1px #00000016 solid;
  cursor: pointer;
}

#hyperpay-body .payment-entity:hover {
  background-color: var(--highlight-color);
}

#hyperpay-body .icon-mada { 
  background-image: url('data:image/png;base64,');
}

#hyperpay-body .icon-visa {
  background-image: url('data:image/png;base64,');
}

#hyperpay-body .icon-mastercard {
	background-image: url('data:image/png;base64,')
 
}
#hyperpay-body .icon-applepay {
  background-image: url('data:image/png;base64,')
}
</style>


<div id="hyperpay-body">
  <div class="payment-title">Please select payment method</div>

  <div class="payment-list">
	  <div class="payment-entity icon-mada" data-entityid="8ac7a4c7803f575f018041f32bfb0791"></div>
	  <div class="payment-entity icon-visa" data-entityid="8ac7a4c7803f575f018041eec7ed0789"></div>
	  <div class="payment-entity icon-mastercard" data-entityid="8ac7a4c7803f575f018041eec7ed0789"></div>
	  <div class="payment-entity icon-applepay" data-entityid="8ac7a4c7803f575f018041f5b720079a"></div>
  </div>

</div>


<script>
$(document).ready(function() {
  $("#hyperpay-body .payment-entity").click(function() {
    var entityId = $(this).attr("data-entityid");
    console.log(entityId);
    asyncLoad("#hyperpay-body", "?page=hyperpay-checkout&entityId=" + entityId + "&SaleId=<%=saleId%>");
  });
});
</script>