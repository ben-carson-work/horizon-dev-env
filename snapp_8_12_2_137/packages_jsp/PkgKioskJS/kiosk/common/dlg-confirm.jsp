<div id="dlg-confirm" class="modal fade" tabindex="-1" data-controller="DlgConfirmController">
  <div class="modal-dialog modal-dialog-centered">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title"></h5>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body">
      </div>
      <div class="modal-footer">
        <button id="btn-yes" type="button" class="btn btn-primary">Yes</button>
        <button id="btn-no" type="button" class="btn btn-danger close-btn" data-bs-dismiss="modal">No</button> 
      </div>
    </div>
  </div>
</div>